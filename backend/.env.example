# Server
PORT=5000
# Allow multiple origins separated by commas
FRONTEND_ORIGIN=http://localhost:5173

# MongoDB Atlas
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-host>/<db-name>?retryWrites=true&w=majority&appName=Cluster0

# Firebase Admin SDK (Service Account)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
# Important: keep quoted; escape newlines as \n
FIREBASE_PRIVATE_KEY=""

# Optional OpenAI key for AI suggestions
OPENAI_API_KEY=

# Dev only: bypass Firebase verification (0 or 1)
DEV_AUTH_BYPASS=0
